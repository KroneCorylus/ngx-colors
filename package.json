{
  "name": "ngx-colors",
  "version": "3.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-lib": "ng build --configuration production ngx-colors",
    "npm-pack": "cd dist/ngx-colors && npm pack",
    "package": "npm run build-lib && npm run npm-pack",
    "publishLib": "cp -f README.md dist/ngx-colors && cp -f README.md projects/ngx-colors && cd dist/ngx-colors && npm publish",
    "XUX": "npm version minor && cd projects/ngx-colors && npm version minor",
    "XXU": "npm version patch && cd projects/ngx-colors && npm version patch",
    "BPP": "npm run package && npm run publishLib #BUILD PACKAGE n PUBLISH",
    "BFD": "ng build --configuration production ngx-color-examples && cd dist/ngx-color-examples && firebase deploy #BUILD n FIREBASE DEPLOY"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.2",
    "@angular/cdk": "~12.2.2",
    "@angular/common": "^12.2.2",
    "@angular/compiler": "^12.2.2",
    "@angular/core": "^12.2.2",
    "@angular/forms": "^12.2.2",
    "@angular/material": "^12.2.2",
    "@angular/platform-browser": "^12.2.2",
    "@angular/platform-browser-dynamic": "^12.2.2",
    "@angular/router": "^12.2.2",
    "ngx-highlightjs": "^4.1.1",
    "npm": "^7.21.0",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.0.3",
    "@angular-devkit/build-ng-packagr": "^0.1001.2",
    "@angular/cli": "^12.2.2",
    "@angular/compiler-cli": "^12.2.2",
    "@angular/language-service": "^12.2.2",
    "@types/jasmine": "^3.5.11",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.53",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.3.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^12.0.3",
    "postcss": "^8.4.5",
    "protractor": "~7.0.0",
    "ts-node": "~8.7.0",
    "tslint": "^6.1.2",
    "typescript": "^4.2.4"
  }
}
